# Environment variables for MCPhub with Vibetest DXT
# Copy this to .env and set your actual API key

# Required: Google API Key for Gemini 2.0 Flash
# Get yours from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Alternative API key names (Vibetest checks all of these)
# GEMINI_API_KEY=your_google_api_key_here
# GOOGLE_GENAI_API_KEY=your_google_api_key_here

# Optional: Vibetest Configuration
ANONYMIZED_TELEMETRY=false
BROWSER_USE_LOGGING_LEVEL=CRITICAL

# Optional: MCPhub Configuration
NODE_ENV=production
PORT=8080
